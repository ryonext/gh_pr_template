{
  "name": "gh_pr_template",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Create template for GitHub pull request.",

  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "app.js"
      ]
    }
  ]
}
