{
  "manifest_version": 2,

  "name": "gh_pr_template",
  "description": "Create template for GitHub pull request.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/compare"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "embeded-script.js"
  ],
  "permissions": [
    "https://github.com/*/compare"
  ]
}
